<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ProdigyProjectFinal.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ProdigyProjectFinal"
    Shell.FlyoutBehavior="Disabled"
    xmlns:view="clr-namespace:ProdigyProjectFinal.View"
    >
    
    <ShellContent 
        Title="MainPage"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage"
    />
    <ShellContent 
    Title="Sign Up"
    ContentTemplate="{DataTemplate view:SignUp}"
    Route="SignUp"
/>
    <ShellContent 
    Title="Login"
    ContentTemplate="{DataTemplate view:Login}"
    Route="Login"/>



</Shell>
